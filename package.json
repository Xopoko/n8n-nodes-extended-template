{
  "name": "n8n-nodes-extended-git",
  "version": "0.1.1",
  "description": "Community nodes for n8n including a Git Extended node.",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Xopoko/n8n-nodes-extended-git",
  "author": {
    "name": "Xopoko",
    "email": "cyprus.horoko@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Xopoko/n8n-nodes-extended-git.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.{ts,tsx}\" package.json",
    "lintfix": "eslint --ext .ts nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json",
    "test": "node --test"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/GitExtended/GitExtended.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.17.47",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
